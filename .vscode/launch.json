  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "run current js",
      "program": "${file}",
      "args": ["--experimental-modules"],
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "run app",
      "program": "${workspaceFolder}/src/app.mjs",
      "args": ["--experimental-modules"],
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node"
    }
  ]
}
